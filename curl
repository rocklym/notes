# 上传文件
curl http://192.168.100.93:10002/upload-config?username=rock -F "file=@C:\\Users\\Administrator\\Desktop\\comp1.txt"  -X POST



# curl form
curl "http://192.168.100.93/userrote" -F "cid=TestVer" -F "uid=122231_9999"
# json
curl "http://localhost:10002/rest/roles" -H "Content-type: application/json" -X POST -d '[{"name":"tester1"}]'
curl "http://localhost:10002/rest/role/id/3" -H "Content-type: application/json" -X PUT -d '[{"name":"tester2"}]'
curl "http://localhost:10002/rest/role/id/3" -H "Content-type: application/json" -X DELETE 

# curl上传文件
